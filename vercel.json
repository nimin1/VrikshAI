{
  "buildCommand": "cd web && npm install && npm run build",
  "outputDirectory": "web/build",
  "installCommand": "npm install && cd web && npm install",
  "devCommand": "cd web && npm start",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/api/darshan",
      "destination": "/api/darshan.js"
    },
    {
      "source": "/api/auth/:path*",
      "destination": "/api/auth.js"
    },
    {
      "source": "/api/vana/:id",
      "destination": "/api/vana.js"
    },
    {
      "source": "/api/vana",
      "destination": "/api/vana.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
