{
  "name": "vriksh-ai",
  "version": "1.0.0",
  "description": "VrikshAI - Ancient Wisdom. Modern Intelligence. Full-stack plant identification and care platform.",
  "private": true,
  "type": "module",
  "dependencies": {
    "@supabase/supabase-js": "^2.80.0",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.75.0"
  },
  "scripts": {
    "install": "cd web && npm install",
    "start": "cd web && npm start",
    "dev": "node dev-server.js",
    "dev:frontend": "cd web && PORT=3001 BROWSER=none npm start",
    "build": "cd web && npm run build",
    "build:frontend": "cd web && npm run build",
    "test": "cd web && npm test",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "vercel-build": "cd web && npm install && npm run build",
    "clean": "rm -rf web/node_modules web/build .vercel",
    "reinstall": "npm run clean && npm run install"
  },
  "keywords": [
    "ai",
    "plants",
    "ayurveda",
    "gpt-4",
    "pwa",
    "vercel",
    "react",
    "python"
  ],
  "author": "VrikshAI Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vriksh-ai"
  },
  "devDependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5"
  }
}
